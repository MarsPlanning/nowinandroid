{
  "extends": [
    "config:base"
  ],
  "includeForks": true,
  "packageRules": [
    {
      "matchDepNames": [
          "libpag"
      ],
      "ignoreUnstable": false,
      "bumpVersion": "prerelease"
    }
  ],
  "regexManagers": [
    {
      "fileMatch": ["^gradle/libs\\.versions\\.toml$"],
      "matchStrings": [
        "libpag = \"(?<currentValue>.*?)\""
      ],
      "packageNameTemplate": "Tencent/libpag",
      "depNameTemplate": "libpag",
      "datasourceTemplate": "github-releases"
    }
  ]
}
