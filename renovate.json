{
  "extends": [
    "config:base"
  ],
  "includeForks": true,
  "packageRules": [
    {
      "matchPackageNames": [
          "tencent:libpag"
      ],
      "enabled": false
    }
  ],
  "regexManagers": [
    {
      "fileMatch": ["^gradle/libs\\.versions\\.toml$"],
      "matchStrings": [
        "libpag = \"(?<currentValue>.*?)\""
      ],
      "depNameTemplate": "Tencent/libpag",
      "datasourceTemplate": "github-releases"
    }
  ]
}
