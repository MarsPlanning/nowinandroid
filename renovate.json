{
  "extends": [
    "config:base"
  ],
  "includeForks": true,
  "regexManagers": [
    {
      "fileMatch": ["^gradle/libs\\.versions\\.toml$"],
      "matchStrings": [
        "libpag = \"(?<currentValue>.*?)\""
      ],
      "depNameTemplate": "Tencent/libpag",
      "datasourceTemplate": "github-releases",
      "ignoreUnstable": false,
      "bumpVersion": "prerelease"
    }
  ]
}
